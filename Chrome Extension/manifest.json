{
  "manifest_version": 2,

  "name": "Customized Notifications Chrome Extension",
  "description": "This is the chrome extension to use the custom notifications applications",
  "version": "1.0",
  "background": {
    "scripts": ["jquery-3.2.1.min.js", "background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "gcm",
    "https://h98rn279xh.execute-api.us-east-1.amazonaws.com/dev/subscribe",
    "storage"
  ]
}
